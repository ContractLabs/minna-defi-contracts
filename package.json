        "@typechain/hardhat": "^6.1.5",
        "@types/mocha": "^10.0.1",
        "@uniswap/v2-core": "^1.0.1",
        "chai": "^4.3.7",
        "cross-env": "^7.0.3",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "ethers": "^5.7.2",
        "fs-extra": "^11.1.0",
        "hardhat": "^2.12.6",
        "hardhat-contract-sizer": "^2.7.0",
        "hardhat-gas-reporter": "^1.0.9",
        "oz-custom": "1.1.9-alpha-1",
